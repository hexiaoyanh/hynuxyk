<!--pages/gatecode/gatecode.wxml-->
<view class="global">
  <view style="margin-top:{{stateH}}px;height:{{stateH}}px;width 5%">
    <navigator style="height:100%;width:100%;padding-left:2%" open-type="navigateBack"><text class="cuIcon-back text-gray" style="font-size:40rpx"></text></navigator>
  </view>

  <view class="card">
    <view class="card-image">
      <image class="main-card-image" src="https://wx4.sinaimg.cn/mw690/006blzWmgy1g7wwp8bw93j30xc0irn11.jpg" mode="aspectFill"></image>
    </view>
    <view class="card-content animated slideInDown">
      <view class="ttitle">      
        <text>
          您的开门密码为
        </text>
      </view>

      <view class="card-content-textblock">
        <text>
          {{pwd}}
        </text>
      </view>
    </view>

    <view class="slogan">
      <text>
        "Can You Fell The Love Tonight,It Is Where We Are."
      </text>
      <view style="text-align:right;margin-top:10rpx">
        <text >———Elton John </text>
      </view>
    </view>
    <ad unit-id="adunit-3d5a3a9a83a04607"></ad>
  </view>

</view>
