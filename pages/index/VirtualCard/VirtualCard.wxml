<view class="global">
    <view style="margin-top:{{stateH}}px;height:{{stateH}}px;width 5%">
    <navigator style="height:100%;width:100%;padding-left:2%" open-type="navigateBack"><text class="cuIcon-back" style="font-size:40rpx"></text></navigator>
  </view>

  <view class="card">
    <view class="card-content animated slideInUp">
      <view class="card-content-main">
        <view class="card-title">
          <text>虚拟校园卡</text>
        </view>
        <view class="card-2Dimage" style="height:{{qrcode_w+18}}px" bindtap="BigPic">
          <canvas style="width:{{qrcode_w}}px; height:{{qrcode_w}}px;" canvas-id='canvas' bindlongtap='save'></canvas>
        </view>
      </view>
    </view>
    <view class="card-image">
      <image class="main-card-image" src="https://wx1.sinaimg.cn/mw690/006blzWmgy1g7wws0wdswj30qa0dcmya.jpg">
      </image>
    </view>
    <view class="card-content-tag animated slideInDown">
      <text>{{UserNumber}} {{AccName}}</text>
    </view>
  </view>
</view> 
